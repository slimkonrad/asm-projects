    30                                  ; This file
    31                                  ;   File name: swap.asm
    32                                  ;   Language: x86-64 Assembly
    33                                  ;   Assemble: nasm -f elf64 -g -F dwarf -o swap.o swap.asm
    34                                  ;   Purpose: A minimal assembly function, called by C, to swap two 64-bit double values in memory.
    35                                  ;========1=========2=========3=========4=========5=========6=========7=========8=========9=========0=========1=========2=========3**
    36                                  
    37                                  global swap
    38                                  
    39                                  section .text
    40                                  swap:
    41                                      ; rdi = pointer to the first double (double *a)
    42                                      ; rsi = pointer to the second double (double *b)
    43                                      
    44 00000000 F20F1007                    movsd   xmm0, [rdi]         ; temp = *a
    45 00000004 F20F100E                    movsd   xmm1, [rsi]         ; use xmm1 to hold *b
    46 00000008 F20F110F                    movsd   [rdi], xmm1         ; *a = *b
    47 0000000C F20F1106                    movsd   [rsi], xmm0         ; *b = temp
    48                                  
    49 00000010 C3                          ret
